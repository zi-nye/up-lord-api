<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="uplord.uplordapi.event.dao.BirthdayDAO">
  <select id="findList" resultType="MemberInfoDTO">
    SELECT
       MEMBER_IDX
     , MEMBER_NAME
     , MEMBER_BIRTH
    FROM MEMBER
    WHERE USE_YN = 'Y'
  </select>
</mapper>
